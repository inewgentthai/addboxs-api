{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "username": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "lastname": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "confirmed": {
      "type": "boolean"
    },
    "blocked": {
      "type": "boolean"
    },
    "status": {
      "type": "boolean"
    }
  },
  "required": ["name"]
}
